.model{
  width:600rpx;
  height:800rpx;
  background:rgba(255,255,255,.8);
  border-radius:20rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 99;
  padding: 60rpx 45rpx;
  font-size: 30rpx;
  color: #080E1A;
  line-height: 52rpx;
}
.model text{
  display: block;
  text-align: center;
  color: #080E1A;
  font-size: 32rpx;
  font-weight: 600;
  margin: 20rpx 0 40rpx;
}
.model navigator{
  width:240rpx;
  height:80rpx;
  background:rgba(255,51,187,1);
  border-radius:40rpx;
  margin:80rpx auto 0;
  color: #FFFFFF;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.model navigator:active{
  background: #cc52a3;
}