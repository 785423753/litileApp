.content{
  position: relative;
  z-index: 99;
  padding: 60rpx 60rpx 0;
  min-height: 100vh;
}
.logo{
  width: 459rpx;
  height: 292rpx;
  display: block;
  margin: 150rpx auto 63rpx; 
}
.get_rule{
  text-align: center;
  color: #B8BECC;
  font-size: 28rpx;
}
.get_rule image{
  width: 24rpx;
  height: 24rpx;
}
.picker{
  color: #B8BCCC;
  width: 100%;
  height: 100rpx;
  background: rgba(255,255,255,.1);
  margin:60rpx auto;
  border-radius: 8rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
  position: relative;
}
.picker image{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 30rpx;
  z-index: 99;
  top: 40rpx;
}
.content button{
  width:280rpx;
  height:88rpx;
  background:rgba(184,188,204,1);
  border-radius:44rpx;
  color: #080E1A;
  font-size: 32rpx;
  margin: 0 auto 75rpx;
  line-height: 88rpx;
}
.content button:active{
  background: #7a8199;
}
.rule{
  color: #B8BCCC;
  font-size: 28rpx;
  text-align: center;
  line-height: 25px;
}

.star{
  width: 50%;
  display: inline-block;
  padding-left: 120rpx;
  color: #B8BCCC;
  font-size: 20rpx;
  position: relative;
  vertical-align: top;
  margin-bottom: 30rpx;
  /* border: 1px solid red; */
}
/* .star:nth-child(odd){
  margin-left: 1%;
} */
.star image{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}
.nr text{
  display: block;
  margin-bottom: 10rpx;
  font-size: 28rpx;
  color: #fff;
  font-weight: bold;
}
.nr .white{
  color: #B8BECC;
  font-size: 24rpx;
  font-weight: 500
}

.share{
  width:280rpx;
  height:88rpx;
  background:#B8BCCC;
  border-radius:44rpx;
  color: #080E1A;
  font-size: 32rpx;
  line-height: 88rpx;
  margin: auto;
  border: 1px solid #080E1A;
  position: fixed;
  bottom: 20px;
  left: 0;
  right: 0;
}