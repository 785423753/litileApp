.list{
  background: #fff;
}
.tag_title{
  padding: 10px;
  font-size: 14px;
}

.shortcut{
  width: 40px;
  height: 40px;
  padding: 7px 8px 0;
  text-align: center;
  border: 1px solid #d9d9d9;
  border-radius: 100%;
  background: #fff;
  font-size: 9px;
  color: #666;
  position: fixed;
  bottom:15px;
  right: 10px;
  z-index: 999;
}
.nav{
  bottom: 65px;
}
.bg{
  position: fixed;
  width: 100%;
  height: 100vh;
  background: rgba(0,0,0,.5);
  z-index: 999;
  top: 0;
  left: 0;
  transition: all .3s ease-out;
  display: none;
}
.short_nav{
  position: fixed;
  bottom: 15px;
  right: 10px;
  height: auto;
  transition: all .3s ease-out;
  z-index: 999;
  overflow: hidden;
  /* height: 0; */
}
.short_nav navigator{
  display: block;
  font-size: 10px;
  color: #fff;
  text-align: center;
  margin-top: 5px;
}
.short_nav view{
  width: 40px;
  height: 40px;
  border-radius: 100%;
  background: #fff;
}
.short_nav image{
  width: 20px;
  height: 20px;
  margin: 10px auto;
  /* display: block; */
}
.short_nav .close{
  margin-top: 20px;
  text-align: center;
}
