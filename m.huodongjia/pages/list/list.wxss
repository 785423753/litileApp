.filter{
  background: #fff;
  height: 40px;
  display: inline-flex;
  width: 100%;
  justify-content: space-between;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999
}
.filter .section{
  width: 33%;
  position: relative;
}
.filter .picker{
  line-height: 40px;
  font-size: 14px;
  text-align: center;
  color: #999;
}
.picker text{
  color: #4BA8EE;
}
.date::before{
    content: "";
    width: 1px;
    height: 20px;
    position: absolute;
    left: 0;
    top: 10px;
    background: #D9D9D9;
}
.date::after{
    content: "";
    width: 1px;
    height: 20px;
    position: absolute;
    right: 0;
    top: 10px;
    background: #D9D9D9;
}

.searh_form{
  position: relative;
  padding: 10px;
  display: block;
  width: 100%;
  box-sizing: border-box;
  background: #f5f5f5;
}
.searh_form input{
  width: 100%;
  height: 35px;
  color: #333;
  font-size: 14px;
  border: none;
  background: #fff;
  border-radius: 15px;
  padding: 0 10px;
}
.searh_form button{
   width: 22px;
  position: absolute;
  right: 20px;
  top: 10px;
  z-index: 99;
  background: none;
  height: 35px;
}
button::after{
  border: none;
}
.searh_form image{
  width: 22px;
  height: 22px;
  position: relative;
  right: 10px;
  bottom: 2px;
}

.list{
  margin-top: 40px;
  background: #fff;
}
