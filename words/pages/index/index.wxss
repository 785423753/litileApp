/**index.wxss**/
.content{
  width: 606rpx;
  margin: auto;
}
.logo{
  width: 150rpx;
  height: 150rpx;
  display: block;
  margin: 100rpx auto;
}
.input{
  width: 100%;
  height: 100rpx;
  background: #fff;
  border: none;
  margin: auto;
  text-align: left;
  font-size: 32rpx;
  padding-left: 76rpx;
  border-radius: 2px;
  color: #1A1A1A;
}
.search{
  position: relative;
  width: 100%;
  margin:0 auto 20rpx;
}
.search image{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  left: 30rpx;
  top: 34rpx;
  z-index: 99;
}
.content button{
  display: inline-block;
  width: 290rpx;
  height: 100rpx;
  border-radius: 2px;
  line-height: 100rpx;
  font-size: 32rpx;
}
.btn_0[disabled]:not([type]){
  background: #3A90A9;
  color: #C2E8F2;
}
.btn_1[disabled]:not([type]){
  background: #7EC4D9;
  color: #2D6C80;
}
.btn_0{
  background: #1F505E;
  color: #FEFEFE;
  margin-right: 20rpx;
}
.btn_1{
  background: #FFF;
  color: #1F505E;
}
.bg{
  position: absolute;
  width: 500rpx;
  bottom: 0;
  margin: auto;
  left: 0;
  right: 0;
}
.history{
  color: #1F505E;
  font-size: 28rpx;
  margin: 60rpx 0 30rpx;
}
.history image{
  width: 28rpx;
  height: 28rpx;
  position: relative;
  top: 1px;
  margin-right: 5px;
}
.word{
  display: inline-block;
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  padding-left: 30rpx;
  color: #1F505E;
  font-size: 28rpx;
  background: #7EC4D9;
  margin-right: 1px;
  margin-bottom: 1px;
  position: relative;
  z-index: 99;
}